{
   "defaultSeverity": "warn",
   "extends": [
      "tslint-config-standard"
   ],
   "linterOptions": {
      "include": [
        "./src/**/*.ts"
      ],
      "exclude": [
        "**/*.spec.ts",
        "**/*.json",
        "./tests/**/*.js",
        "*/node_modules/**/*"
      ]
  },
  "jsRules": {},
  "rules": {
    "strict-type-predicates": false
  },
  "rulesDirectory": []
}
