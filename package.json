{
  "name": "awesome-vue-admin",
  "version": "0.1.0",
  "private": true,
  "description": "A awesome admin system powered by vue and electron",
  "author": "linuxing3",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "tslint": "tslint -p . --fix",
    "dev": "vue-cli-service electron:serve",
    "dist": "cross-env NODE_ENV=production vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "electron:build": "vue-cli-service electron:build",
    "gen": "hygen gen",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "test:unit": "vue-cli-service test:unit",
    "test:uw": "vue-cli-service test:unit --watch",
    "scripts:debug": "node --nolazy --inspect-brk=9229 ./scripts/bin/fp.js",
    "scripts:translate": "node ./scripts/bin/translate.js",
    "scripts:watch": "tsc -p ./scripts --watch",
    "scripts:debug:ts": "ts-node ./scripts/fp.ts",
    "storybook:serve": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "inspect": "vue inspect > ./webpack.config.inspect.js"
  },
  "dependencies": {
    "@vuex-orm/core": "^0.31.4",
    "ant-design-vue": "^1.3.8",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "csv": "^5.1.1",
    "docx": "^4.7.0",
    "echarts": "^4.1.0",
    "filepond": "^4.2.0",
    "lodash": "^4.17.11",
    "lowdb": "^1.0.0",
    "papaparse": "^4.6.3",
    "rxjs": "^6.4.0",
    "vue": "^2.6.8",
    "vue-class-component": "^6.0.0",
    "vue-crud-x": "^0.1.9",
    "vue-echarts": "^4.0.1",
    "vue-filepond": "^5.0.0",
    "vue-i18n": "^8.0.0",
    "vue-property-decorator": "^7.0.0",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.1",
    "vue-rx": "^6.1.0",
    "vuetify": "^1.3.0",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1",
    "vuex-orm-localforage": "^0.1.5",
    "vuex-orm-lowdb": "^0.3.4",
    "vuex-pathify": "^1.1.3",
    "vuex-persist": "^2.0.0",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@storybook/addon-actions": "^5.0.3",
    "@storybook/addon-links": "^5.0.3",
    "@storybook/addon-storyshots": "^5.0.3",
    "@storybook/addons": "^5.0.3",
    "@storybook/vue": "^5.0.3",
    "@types/jest": "^23.1.4",
    "@types/lodash": "^4.14.122",
    "@types/webpack": "^4.4.0",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-typescript": "^3.4.0",
    "@vue/cli-plugin-unit-jest": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-preset-vue": "^2.0.2",
    "cross-env": "^5.2.0",
    "electron": "^3.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^1.3.1",
    "hygen": "^2.1.2",
    "json-format": "^1.0.1",
    "lint-staged": "^8.1.5",
    "material-design-icons-iconfont": "^4.0.3",
    "md5": "^2.2.1",
    "normalizr": "^3.3.0",
    "roboto-fontface": "^0.10.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^23.0.0",
    "tslint": "^5.14.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "~3.2.2",
    "vue-cli-plugin-electron-builder": "^1.0.2",
    "vue-cli-plugin-i18n": "^0.5.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "2.6.8",
    "vue-tsx-support": "^2.2.2",
    "vuetify-loader": "^1.0.5",
    "vuetify-tsx": "^0.24.0"
  },
  "main": "background.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{json,css,md,yml,js,ts}": [
        "yarn run prettier --write",
        "git add"
      ],
      "*.{js,jsx,ts,tsx,vue}": [
        "yarn lint --fix",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/*",
      "**/dist_electron/*",
      "**/node_modules/**/*"
    ]
  }
}
